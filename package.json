{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "warmup": "node scripts/warmup-api.js",
    "build": "tsc -b && vite build && node scripts/warmup-api.js && node scripts/prerender.js && node scripts/prerender-blog.js && node scripts/prerender-services.js && node scripts/generate-sitemap.js",
    "build:simple": "tsc -b && vite build && node scripts/generate-sitemap.js",
    "build:prod": "tsc -b && vite build --mode production && node scripts/warmup-api.js && node scripts/prerender.js && node scripts/prerender-blog.js && node scripts/prerender-services.js && node scripts/generate-sitemap.js",
    "prerender:blog": "node scripts/prerender-blog.js",
    "prerender:services": "node scripts/prerender-services.js",
    "generate:sitemap": "node scripts/generate-sitemap.js",
    "generate:favicon": "node scripts/generate-favicon.js",
    "build:debug": "tsc -b && VITE_DEBUG_API=true vite build --mode development",
    "lint": "eslint .",
    "preview": "vite preview",
    "test:api": "echo 'Testing API connection...' && node -e \"import('./src/utils/debugApi.js').then(m => m.testApiConnection())\"",
    "debug:blog": "echo 'Debugging Blog Module...' && node -e \"console.log('Environment vars:'); console.log('VITE_API_URL:', process.env.VITE_API_URL); console.log('VITE_BACKEND_URL:', process.env.VITE_BACKEND_URL);\""
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.52.0",
    "@hookform/resolvers": "^5.2.2",
    "@tiptap/extension-color": "^3.7.1",
    "@tiptap/extension-font-family": "^3.7.1",
    "@tiptap/extension-highlight": "^3.11.0",
    "@tiptap/extension-image": "^3.11.0",
    "@tiptap/extension-link": "^3.7.1",
    "@tiptap/extension-placeholder": "^3.11.0",
    "@tiptap/extension-table": "^3.11.0",
    "@tiptap/extension-table-cell": "^3.11.0",
    "@tiptap/extension-table-header": "^3.11.0",
    "@tiptap/extension-table-row": "^3.11.0",
    "@tiptap/extension-text-align": "^3.7.1",
    "@tiptap/extension-text-style": "^3.7.1",
    "@tiptap/extension-underline": "^3.7.1",
    "@tiptap/react": "^3.7.1",
    "@tiptap/starter-kit": "^3.7.1",
    "@types/dompurify": "^3.0.5",
    "@types/marked": "^5.0.2",
    "axios": "^1.12.2",
    "browser-image-compression": "^2.0.2",
    "dompurify": "^3.3.0",
    "lucide-react": "^0.552.0",
    "marked": "^17.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.65.0",
    "react-router-dom": "^7.9.4",
    "recharts": "^3.3.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vercel/edge": "^1.2.2",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "png-to-ico": "^3.0.1",
    "postcss": "^8.5.6",
    "react-snap": "^1.23.0",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^6.0.6"
  },
  "overrides": {
    "react-helmet-async": {
      "react": "^19.1.1",
      "react-dom": "^19.1.1"
    }
  },
  "resolutions": {
    "react-helmet-async": "^2.0.5"
  }
}
