# Variables de entorno para producción - BLOG MODULE
VITE_API_URL=https://web-scuticompany-back.onrender.com/api
VITE_BACKEND_URL=https://web-scuticompany-back.onrender.com
VITE_CLERK_PUBLISHABLE_KEY=pk_live_tu_key_de_produccion

# Configuración de modo para producción
NODE_ENV=production
VITE_NODE_ENV=production

# Blog Module Configuration - Producción
VITE_BLOG_API_TIMEOUT=30000
VITE_BLOG_RETRY_ATTEMPTS=3
VITE_BLOG_CACHE_TTL=300000

# AI Module Configuration
VITE_AI_MODULE_ENABLED=true
VITE_AI_TIMEOUT=30000
