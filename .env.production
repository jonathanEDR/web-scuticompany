# Features Flags
VITE_ENABLE_AI_ANALYSIS=true
VITE_ENABLE_TAG_GENERATION=true
VITE_ENABLE_SEO_OPTIMIZATION=true

# Debug en producción (comentar para deshabilitar)
# VITE_DEBUG_API=false